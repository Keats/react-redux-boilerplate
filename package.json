{
  "name": "react-redux-boilerplate",
  "version": "0.0.0",
  "description": "A react/redux boilerplate",
  "scripts": {
    "clean": "rm -rf dist && rm -rf build",
    "build": "npm run clean && npm run lint && npm run sass && webpack",
    "dist": "npm run clean && npm run lint && NODE_ENV=production npm run sass && npm run webpack:prod",
    "lint": "eslint src",
    "start": "./bin/dev",
    "test": "karma start --single-run",
    "test-watch": "karma start",
    "sass": "gulp sass",
    "sass-watch": "gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Keats/react-redux-boilerplate.git"
  },
  "author": "Vincent Prouillet",
  "bugs": {
    "url": "https://github.com/Keats/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/Keats/react-redux-boilerplate#readme",
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.0.5",
    "babel-runtime": "^5.8.20",
    "chai": "^3.2.0",
    "eslint": "^1.0.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.2.2",
    "exports-loader": "^0.6.2",
    "express": "^4.13.3",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^3.0.1",
    "gulp-sass": "^2.1.0",
    "html-webpack-plugin": "^2.0.4",
    "imports-loader": "^0.6.4",
    "karma": "^0.13.8",
    "karma-chai": "^0.1.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "react-addons-test-utils": "^0.14.1",
    "react-transform-hmr": "^1.0.0",
    "redux-devtools": "^2.1.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.11.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  },
  "dependencies": {
    "classnames": "^2.1.5",
    "es6-promise": "^3.0.2",
    "history": "^1.12.3",
    "immutable": "^3.7.4",
    "lodash.debounce": "^3.1.1",
    "react": "^0.14.1",
    "react-dom": "^0.14.1",
    "react-redux": "^4.0.0",
    "react-router": "1.0.0-rc3",
    "redux": "^3.0.0",
    "redux-router": "1.0.0-beta3",
    "redux-thunk": "^1.0.0",
    "uuid": "^2.0.1",
    "whatwg-fetch": "^0.10.0"
  }
}
